{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type":"package",
            "package": {
                "name": "eywek/obsifight",
                "version":"1.0.0",
                "source": {
                    "url": "https://github.com/Eywek/obsifight-libs.git",
                    "type": "git",
                    "reference":"master"
                }
            }
        },
        {
            "type":"package",
            "package": {
                "name": "xpaw/ping",
                "version":"1.0.0",
                "source": {
                    "url": "https://github.com/xPaw/PHP-Minecraft-Query.git",
                    "type": "git",
                    "reference":"master"
                }
            }
        },
        {
            "type":"package",
            "package": {
                "name": "urb/xenforobridge",
                "version":"1.0.1",
                "source": {
                    "url": "https://github.com/Eywek/XenforoBridge.git",
                    "type": "git",
                    "reference":"master"
                },
                "autoload": {
                    "psr-4" : {
                        "Urb\\XenforoBridge\\" : "src/XenforoBridge"
                    }
                }
            }
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "eywek/obsifight": "1.0.0",
        "fahim/laravel5-paypal-ipn": "1.0.*",
        "google/apiclient": "^2.0",
        "graham-campbell/markdown": "^7.1",
        "labymod/discord-webhooks": "^1.1",
        "laravel/framework": "5.4.*",
        "laravel/tinker": "~1.0",
        "phpgangsta/googleauthenticator": "dev-master",
        "sebastianwalker/paysafecard-php-api": "^1.0",
        "urb/xenforobridge": "1.0.1",
        "waavi/recaptcha": "1.0.x",
        "webpatser/laravel-uuid": "2.*",
        "xpaw/ping": "1.0.0",
        "yajra/laravel-datatables-oracle": "~7.0",
        "zizaco/entrust": "5.2.x-dev"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7",
        "barryvdh/laravel-debugbar": "^2.3",
        "barryvdh/laravel-ide-helper": "^2.4"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
